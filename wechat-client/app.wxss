page {
    background-color: #ededef;
}

.wrap {
    margin: 0 20rpx;
}

.clearBox {
    zoom:1;
}

.clearBox:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

@font-face {
  font-family: "iconfont";
  src: url('http://www.futunn.com/css/iconfont/iconfont.ttf?t=1471940574') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('http://www.futunn.com/css/iconfont/iconfont.svg?t=1471940574#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
}

.icon-xiaolian:before {
    content: "\e61f";
}

